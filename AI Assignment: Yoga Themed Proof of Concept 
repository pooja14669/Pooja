      "source": [
        " # Sentiment Analysis (NLP)\n",
        "\n",
        " !pip install transformers  # Install transformers library (version 4.31.0)\n",
        "from transformers import pipeline\n",
        "\n",
        "# Load a pre-trained sentiment analysis model\n",
        "classifier = pipeline(\"sentiment-analysis\")\n",
        "\n",
        "def analyze_mood(text):\n",
        "  \"\"\"Analyzes the sentiment of the input text and infers the user's mood.\"\"\"\n",
        "  result = classifier(text)[0]\n",
        "  mood = result['label']  # Extract the predicted mood label (e.g., 'POSITIVE', 'NEGATIVE')\n",
        "  return mood\n",
        "\n",
        "# Example usage:\n",
        "user_text = \"I feel relaxed and calm today.\"\n",
        "predicted_mood = analyze_mood(user_text)\n",
        "print(f\"Predicted mood: {predicted_mood}\") # To see the output, run the code."
      ],
