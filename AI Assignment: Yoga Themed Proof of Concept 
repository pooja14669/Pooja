        "# Create input text box and output display\n",
        "text_input = widgets.Text(placeholder=\"Enter your mood description\")\n",
        "output_display = widgets.Output()\n",
        "\n",
        "# Define a function to handle user input\n",
        "def on_text_change(change):\n",
        "  \"\"\"Updates the recommended routine based on user input.\"\"\"\n",
        "  with output_display:\n",
        "    output_display.clear_output()\n",
        "    mood = analyze_mood(change['new'])\n",
