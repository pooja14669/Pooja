      "cell_type": "code",
      "source": [
        "from transformers import AutoModelForSequenceClassification, AutoTokenizer\n",
        "\n",
        "# Load a more specialized emotion recognition model\n",
        "model_name = \"bhadresh-savani/bert-base-uncased-emotion\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "model = AutoModelForSequenceClassification.from_pretrained(model_name)\n",
        "\n",
        "def analyze_mood(text):\n",
        "    inputs = tokenizer(text, return_tensors=\"pt\")\n",
        "    outputs = model(**inputs)\n",
        "    # Process outputs to extract the most likely emotion\n",
        "    # Assuming 'outputs' contains logits, get the predicted class\n",
        "    predicted_class_idx = outputs.logits.argmax(-1).item()\n",
        "\n",
        "    # Map the class index to an emotion label (e.g., 'POSITIVE', 'NEGATIVE')\n",
        "    # You'll need to adjust this mapping based on your model's output\n",
        "    emotion_labels = ['anger', 'disgust', 'fear', 'joy', 'neutral', 'sadness', 'surprise']\n",
        "    predicted_emotion = emotion_labels[predicted_class_idx]\n",
        "\n",
        "    # Map emotions to 'POSITIVE' or 'NEGATIVE' for simplicity\n",
        "    if predicted_emotion in ['joy', 'surprise']:  # Consider adding more positive emotions if needed\n",
        "        predicted_mood = \"POSITIVE\"\n",
        "    else:\n",
        "        predicted_mood = \"NEGATIVE\"\n",
        "\n",
        "    return predicted_mood  # Return the predicted mood"
      ],
      "metadata": {
        "id": "ESpTNBgUIINR"
      },
      "execution_count": null,
      "outputs": []
    },
